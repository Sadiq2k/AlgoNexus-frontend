{
  "name": "algo-nexus-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:AlgoNexus-Frontend": "node dist/algo-nexus-frontend/server/server.mjs",
    "api-gen": "ng-openapi-gen --input ./src/openapi/openapi.json --output ./src/app/services",
    "api-gen-user": "ng-openapi-gen --input ./src/openapi-user/openapi-user.json --output ./src/app/user-services",
    "api-gen-problem": "ng-openapi-gen --input ./src/openapi-problem/openapi-problem.json --output ./src/app/problem-services",
    "api-gen-testcase": "ng-openapi-gen --input ./src/openapi-testcase/openapi-testcase.json --output ./src/app/testcase-services",
    "api-gen-courses": "ng-openapi-gen --input ./src/openapi-courses/openapi-courses.json --output ./src/app/courses-services",
    "api-gen-dailyProblem":"ng-openapi-gen --input ./src/openapi-dailyProblem/openapi-dailyProblem.json --output ./src/app/dailyProblem-services"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.3.5",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/material": "^17.3.5",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/ssr": "^17.1.0",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@materia-ui/ngx-monaco-editor": "^6.0.0",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngrx/store-devtools": "^17.2.0",
    "@ngx-prism/core": "^2.0.1",
    "@types/readline-sync": "^1.4.8",
    "angular-code-input": "^2.0.0",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.2",
    "express": "^4.18.2",
    "flowbite": "^2.3.0",
    "monaco-editor": "^0.48.0",
    "ng-openapi-gen": "^0.51.0",
    "ng2-charts": "^6.0.0",
    "ngx-bootstrap": "^12.0.0",
    "ngx-toastr": "^18.0.0",
